{
  "name": "barcode-react-scanner",
  "version": "1.1.0",
  "description": "react component to scan barcode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && coveralls < ./coverage/lcov.info",
    "prepublish": "tsc"
  },
  "keywords": [
    "react",
    "component",
    "scanner",
    "barcode",
    "scan"
  ],
  "author": "Adrian Mendez <mendezadrian149@gmail.com>",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@zxing/library": "^0.17.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.51", 
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.5",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.5.2",
    "react-dom": "^16.13.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdrianMendez1199/react-barcode-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/AdrianMendez1199/react-barcode-scanner/issues"
  },
  "homepage": "https://github.com/AdrianMendez1199/react-barcode-scanner#readme"
}